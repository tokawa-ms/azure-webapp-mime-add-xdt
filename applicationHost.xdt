      <mimeMap fileExtension=".ogv" mimeType="video/ogg" xdt:Transform="Insert" />
      <mimeMap fileExtension=".otf" mimeType="font/otf" xdt:Transform="Insert" />
      <mimeMap fileExtension=".spx" mimeType="audio/ogg" xdt:Transform="Insert" />
